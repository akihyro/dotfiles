Host github.com
    CheckHostIP no
    IdentityFile ~/.ssh/github
    StrictHostKeyChecking yes
    User git
    UserKnownHostsFile ~/.ssh/github.hosts

Host cyberz-adagency.github.com
    CheckHostIP no
    HostName github.com
    IdentityFile ~/.ssh/cyberz-adagency.github
    StrictHostKeyChecking yes
    User git
    UserKnownHostsFile ~/.ssh/github.hosts

Host *
    AddKeysToAgent yes
    IdentitiesOnly yes
    ServerAliveCountMax 5
    ServerAliveInterval 60
